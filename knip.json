{
    "$schema": "https://unpkg.com/knip@latest/schema.json",
    "entry": [
        "frontend/react/App.tsx",
        "server/index.ts"
    ],
    "project": [
        "frontend/react/**/*.{ts,tsx}",
        "server/**/*.ts",
        "types/**/*.ts"
    ],
    "ignoreDependencies": [
        "@types/css-modules"
    ],
    "ignoreBinaries": [],
    "rules": {
        "dependencies": "error",
        "devDependencies": "warn",
        "exports": "warn",
        "files": "error",
        "nsExports": "error",
        "types": "warn",
        "unlisted": "error"
    }
}